		    "" == vm.car.strArriveTime ? (alert("请输入入场时间"), $("#download").removeAttr("href")) : ($("#download").attr("href", baseURL + "vehicleentrydetails/zombieDownload?strArriveTime=" + vm.car.strArriveTime), $("#download").attr("download", "无进场信息车辆表"))
		}

